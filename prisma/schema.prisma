generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Mobil {
  id        Int      @id @default(autoincrement())
  nama     String   @db.VarChar(255)
  merk   String?
  harga   Decimal?  @db.Decimal(10, 2)
  kursi   Int?
  bahan_bakar   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("mobil")
}